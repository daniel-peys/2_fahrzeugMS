@startuml KundeUserModel

skinparam classAttributeIconSize 0
' skinparam monochrome true

package org.springframework.security.core.userdetails {
    class User
}

package com.acme.kunde {
    package security {
        CustomUser -up-|> User

        class CustomUser
    }
    package rest #DDDDDD {
        class Umsatz {
            - betrag: BigDecimal
            - waehrung: Currency
        }

        class Adresse {
            - plz: String
            - ort: String
        }

        enum GeschlechtType {
            MAENNLICH
            WEIBLICH
            DIVERS
        }

        enum FamilienstandType {
            LEDIG
            VERHEIRATET
            GESCHIEDEN
            VERWITWET
        }

        enum InteresseType {
            SPORT
            LESEN
            REISEN
        }

        class KundeUserModel << entity >> {
            - nachname : String
            - email : String
            - kategorie: Int
            - newsletter : Boolean
            - geburtsdatum: LocalDate
            - homepage: URL
        }

        KundeUserModel *--> "1" Umsatz : umsatz
        KundeUserModel *--> "1" Adresse : adresse
        KundeUserModel *--> "1" GeschlechtType : geschlecht
        KundeUserModel *--> "1" FamilienstandType : familienstand
        KundeUserModel *--> "*" InteresseType : interessen
        KundeUserModel *-right-> "1" CustomUser : customUser
    }
}

hide empty members

footer (c) JÃ¼rgen Zimmermann

@enduml
